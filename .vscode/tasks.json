{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "0.1.0",
    "command": "npm",
    "isShellCommand": true,
    "showOutput": "always",
    "suppressTaskName": true,
    "tasks": [
        {
            "taskName": "build",
            "args": ["run", "build"]
        },
        {
          "taskName": "package:prod",
          "args": [ "run", "package:prod" ]
        },
        {
          "taskName": "start",
          "args": [ "run", "start" ]
        },
        {
          "taskName": "sw",
          "args": [ "run", "sw" ]
        },
        {
          "taskName": "static-serve",
          "args": [ "run", "static-serve" ]
        }
    ]
}
